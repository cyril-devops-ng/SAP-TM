MODULE user_command_0100 INPUT.
  CASE sy-ucomm.
    WHEN 'BACK' OR '%EX' OR 'RW' OR 'E'
      OR 'ENDE' OR 'ECAN' OR 'EXIT' OR 'CANCEL'.
      LEAVE TO current transaction.
  ENDCASE.
ENDMODULE.
